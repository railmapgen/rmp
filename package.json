{
    "homepage": "https://railmapgen.github.io/rmp",
    "name": "rmp",
    "repository": "github:railmapgen/rmp",
    "bugs": {
        "url": "https://github.com/railmapgen/rmp/issues"
    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "@chakra-ui/react": "^2.4.3",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@railmapgen/rmg-components": "^7.1.5",
        "@railmapgen/rmg-runtime": "^6.0.3",
        "@railmapgen/rmg-palette-resources": "^0.7.15",
        "@railmapgen/rmg-translate": "^2.0.3",
        "@reduxjs/toolkit": "^1.9.1",
        "bezier-js": "^6.1.0",
        "electron-is-dev": "^2.0.0",
        "framer-motion": "^7.6.19",
        "graphology": "^0.25.1",
        "graphology-types": "^0.24.5",
        "i18next-browser-languagedetector": "^6.1.4",
        "nanoid": "^4.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^12.1.1",
        "react-icons": "^4.7.1",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.4.5",
        "react-use-event-hook": "^0.9.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/bezier-js": "^4.1.0",
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.10",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-mock-store": "^1.0.3",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "cross-env": "^7.0.3",
        "electron": "^22.0.0",
        "electron-builder": "^23.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "prettier": "^2.8.1",
        "react-scripts": "^5.0.1",
        "redux-mock-store": "^1.5.4",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:no-watch": "cross-env CI=true react-scripts test --env=jsdom",
        "lint": "eslint ./src",
        "lint:dev": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron-package": "npx electron-builder build -c.extraMetadata.main=build/electron.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "3.0.6",
    "main": "public/electron.js",
    "build": {
        "publish": [
            {
                "provider": "github",
                "vPrefixedTagName": false
            }
        ],
        "snap": {
            "publish": "github"
        }
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*)|(nanoid))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}
